{
  "name": "bonditka/export1c",
  "description": "Generator Xml for 1c.",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "export1c"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ekaterina Sergeeva",
      "email": "ekaterina@sergeeva.site",
      "homepage": "http://www.sergeeva.site"
    }
  ],
  "support": {
    "issues": "https://github.com/bonditka/export1c/issues?state=open",
    "source": "https://github.com/bonditka/export1c"
  },
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "bonditka/yii2-task": "*"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.0",
    "yiisoft/yii2-faker": "~2.0.0",

    "codeception/codeception": "~2.2.0",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3"
  },
  "autoload": {
    "psr-4": {
      "bonditka\\export1c\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bonditka\\export1c\\tests\\": "tests/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}